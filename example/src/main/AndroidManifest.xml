<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.doitlite.taglayoutmanager.example">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".demo1.Demo1Activity"></activity>

        <activity android:name=".demo2.Demo2Activity"></activity>

        <activity android:name=".demo3.Demo3Activity"></activity>

        <activity android:name=".demo4.Demo4Activity"></activity>

    </application>

</manifest>